sseg segment para stack 'stack'
     dw 200h
sseg ends
dseg segment para 'data'
     val1  db  ?
     val2  db  80
     val3  db  5,10,15
     val4 db ?
dseg ends
cseg segment para 'code'
     assume cs:cseg,ds:dseg,ss:sseg
     begin proc far
     mov val1,5
     mov ax,5
     mov ds,ax
     mov val1,4
     mov ax,10
     mov val2,ah
     mov dl,30
     mov bh,val3
     mov val4,bh
     mov ah,val4
     int 21h
begin endp
cseg ends
     end begin
